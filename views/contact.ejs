  <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@4/dist/email.min.js"></script>

  
  <script type="text/javascript">
    
    document.getElementById('gotogmail').addEventListener('click', function() {
        window.location.href = 'https://mail.google.com/mail/u/0/#inbox';
    });
    
      // Initialize EmailJS
      (function() {
          emailjs.init("Y6ExWZw25_nqGHux4");
      })();

      function SendMail() {
          // Get values from the form
          const name = document.getElementById("user_name").value;
          const email = document.getElementById("user_email").value;
          const message = document.getElementById("message").value;

          // Check if the email is a valid Gmail address
          var gmailPattern = /^[a-zA-Z0-9._%+-]+@gmail\.com$/;

          if (!gmailPattern.test(email)) {
              // If the email is not a Gmail address, show an alert and stop the function
              alert("Please enter a valid Gmail address.");
              return;
          }

          // Prepare parameters for the EmailJS service
          var params = {
              from_name: name,
              email_id: email,
              message: message
          };

          // Send the email using EmailJS
          emailjs.send("service_yk1yq8j", "template_main", params)
              .then(function(res) {
                  document.getElementById("successMessage").style.display = "block";
                  document.querySelector(".contact-form").style.display = "none";
                  document.querySelector(".form-title").style.display = "none";
                  document.getElementById("gotogmail").style.display = "block";
              })
              .catch(function(err) {
                  alert("FAILED. Error: " + JSON.stringify(err));
              });
      }
  </script>

	
